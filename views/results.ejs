<% include partials/header %>

<div class="container">
    <h1>Results:</h1>
</div>
<div class="container">    
    <% data["Search"].forEach(function(movie) { %>
        <div class="col-md-4">
            <div class="result" id="<%=movie["imdbID"]%>">   
               <h2><%= movie["Title"] %> (<%= movie["Year"] %>)</h2>
               <img src="<%= movie["Poster"] %>"></img>
            </div>
        </div>
    <% }); %>
</div>

<% include partials/footer %>